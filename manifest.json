{
  "manifest_version": 2,

  "name": "FGJ",
  "description": "Chrome extension hackathon",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
   "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery.min.js", "insert.js"]
  }
]
}
